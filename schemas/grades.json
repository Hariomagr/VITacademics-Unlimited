{
  "id": "/Grades",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "grades": { "type": "array", "items": { "$ref": "GradeDetail" }, "required": "true"  },
    "semester_wise": { "type": "array", "minItems": 1, "items": { "$ref": "SemesterGradeDetail" }, "required": "true"  },
    "grade_count": { "type": "array", "minItems": 1, "items": { "$ref": "GradeCount" }, "required": "true"  },
    "credits_registered": { "type": "number", "required": "true"  },
    "credits_earned": { "type": "number", "required": "true"  },
    "cgpa": { "type": "number", "required": "true"  }
  }
}
